#include<iostream>
#include<climits>
using namespace std;
int main(){
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++){
		cin>>a[i];
		
	}
	int max=INT_MIN;//array 0 ki value maine max variable m store kr di 
	for(int i=0;i<n;i++){
		if(a[i]>max){
			max=a[i];
		}
	}
	cout<<"maximum number is ="<<max<<endl;
	return 0;
}
